---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";
import Pricing from "../components/global/Pricing.astro";
import Testimonial from "../components/global/Testimonial.astro";
import Faq from "../components/global/Faq.astro";
import Cta from "../components/global/Cta.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="Home"
  description="Your site description goes here"
  canonical="https://www.unwraped.design"
  openGraph={{
    url: "https://www.unwraped.design",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://www.unwraped.design/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.unwraped.design/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.unwraped.design/opengraph/social-og.jpg" },
      { url: "https://www.unwraped.design/opengraph/twitter-og.jpg" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@TwitterHandle",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <Navigation />
  <Pricing />
  <Testimonial />
  <Faq />
  <Cta />
</BaseLayout>
